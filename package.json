{
  "name": "LPCE-Webapps-Server ",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "posttest": "npm run lint",
    "dev:server": "NODE_ENV=development nodemon --ignore mydata.json --ignore client/dist/ --ignore client/ server/server.js --watch",
    "prod:server": "NODE_ENV=production nodemon --ignore mydata.json --ignore client/dist/ --ignore client/ server/server.js --watch",
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js /var/www/node/develop/lpce-webapps-client/src/app/shared/sdk -d ng2web -i  disabled",
    "deploy:server": "rsync -arv --exclude 'client' --exclude 'node_modules' --exclude 'log' --exclude '.git' --exclude 'db.json' --exclude 'mydata.json' --exclude 'ecosystem.config.js' --exclude 'server/log' -n /var/www/node/develop/lpce-webapps-server/ /var/www/node/production/lpce-webapps/",
    "deploy:server:real": "rsync -arv --exclude 'client' --exclude 'node_modules' --exclude 'log' --exclude '.git' --exclude 'db.json' --exclude 'mydata.json' --exclude 'ecosystem.config.js' --exclude 'server/log' /var/www/node/develop/lpce-webapps-server/ /var/www/node/production/lpce-webapps/"
  },
  "dependencies": {
    "@mean-expert/loopback-sdk-builder": "^2.3.1",
    "@types/socket.io-client": "^1.4.32",
    "cls-hooked": "^4.2.2",
