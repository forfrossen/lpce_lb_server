    "express-flash": "0.0.2",
    "express-kerberos": "^1.0.0",
    "express-session": "^1.17.0",
    "ftp": "^0.3.10",
    "helmet": "^3.16.0",
    "ipp": "^2.0.1",
    "jade": "^1.7.0",
    "jsonwebtoken": "^8.5.0",
    "loopback": "^3.25.1",
    "loopback-boot": "^2.27.1",
    "loopback-component-explorer": "^6.3.1",
    "loopback-component-passport": "^3.11.0",
    "loopback-connector-ldap": "=2.0.27",
    "loopback-connector-mssql": "^3.3.0",
    "loopback-connector-oracle": "^4.1.2",
    "loopback-connector-sendmail": "0.0.2",
    "loopback-context": "^3.4.0",
