{
  "name": "accessToken",
  "plural": "accessTokens",
  "base": "AccessToken",
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "ttl": {
      "type": "number",
      "default": 86400
    }
  },
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
	{
		"accessType": "*",
		"principalType": "ROLE",
		"principalId": "$authenticated",
		"permission": "ALLOW"
    }],
  "methods": {}
}
