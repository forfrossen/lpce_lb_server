{
	"initial:before": {
		"loopback#favicon": {}
	},
	"initial": {
		"loopback-context#per-request": {
			"params": {
				"enableHttpContext": true
			}
		},
		"compression": {},
		"cors": {
			"params": {
				"origin": true,
				"credentials": true,
				"maxAge": 86400
			}
		},
		"helmet#xssFilter": {},
		"helmet#frameguard": {
			"params": {
				"action": "deny"
			}
		},
		"helmet#hsts": {
			"params": {
				"maxAge": 0,
				"includeSubdomains": true
			}
		},
		"helmet#hidePoweredBy": {},
		"helmet#ieNoOpen": {},
		"helmet#noSniff": {},
		"helmet#noCache": {
			"enabled": false
		}
	},
	"session:before": {},
	"session": {},
	"auth": {
		"loopback#token": { 
			"params": {
                "currentUserLiteral": "me"
            }
		}
	},
	"auth:after": {
		"./middleware/store-current-user.js": {}
	},
	"parse": {},
	"routes": {
		"loopback#rest": {
			"paths": [
				"${restApiRoot}"
			]
		}
	},
	"files": {
		"loopback#static": {
			"params": "$!../client/dist"
		}
	},
	"files:after": {
		"./middleware/myRouting.js": {}
	},
	"final": {
		"loopback#urlNotFound": {}
	},
	"final:after": {
		"strong-error-handler": {
			"params": {
				"safeFields": [
					"errorCode"
				]
			}
		}
	}
}